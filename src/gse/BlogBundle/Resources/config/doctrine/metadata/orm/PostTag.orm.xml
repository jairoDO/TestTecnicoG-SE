<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PostTag" table="post_tag">
    <indexes>
      <index name="fk_post_tag_post_idx" columns="post_id"/>
      <index name="fk_post_tag_tag1_idx" columns="tag_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="tag" target-entity="Tag">
      <join-columns>
        <join-column name="tag_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="post" target-entity="Post">
      <join-columns>
        <join-column name="post_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
